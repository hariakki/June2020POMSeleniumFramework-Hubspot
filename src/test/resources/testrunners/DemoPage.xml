<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Hub spot Test automtion regression suite" verbose="3">
	<!-- parallel="tests" thread-count="3" -->


	<listeners>

		<listener
			class-name="com.qa.hubspot.listeners.ExtentReportListener" />

	</listeners>


	<test name="Demo page Test : faltu ">
		<parameter name="browser" value="chrome" />
		<parameter name="url" value="https://app.hubspot.com/" />
		<classes>
			<class name="com.qa.hubspot.tests.DemoPageTest" />
		</classes>
	</test>

</suite>